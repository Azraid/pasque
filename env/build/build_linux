go build -o $GOPATH/bin/pasque/router $GOPATH/src/pasque/server/router/main.go $GOPATH/src/pasque/server/router/router.go
go build -o $GOPATH/bin/pasque/svcgate $GOPATH/src/pasque/server/svcgate/main.go $GOPATH/src/pasque/server/svcgate/gate.go
go build -o $GOPATH/bin/pasque/apigate $GOPATH/src/pasque/server/apigate/main.go $GOPATH/src/pasque/server/apigate/gate.go

go build -o $GOPATH/bin/pasque/hellosrv $GOPATH/src/pasque/server/hellosrv/main.go
go build -o $GOPATH/bin/pasque/hellocli $GOPATH/src/pasque/server/hellocli/main.go

mkdir $GOPATH/bin/pasque/test
mkdir $GOPATH/bin/pasque/config

cp -rf $GOPATH/src/pasque/env/config/system_linux.json $GOPATH/bin/pasque/config/system.json
cp -rf $GOPATH/src/pasque/env/config/db.json $GOPATH/bin/pasque/config/db.json
cp -rf $GOPATH/src/pasque/run_linux $GOPATH/bin/pasque/run
